<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4">Real Estate Rental System</h1>
        <p class="lead">Real Estate Rental Management System is a web-based system to facilitate flatshare and
            finding roommates.
        </p>
    </div>
</div>

<table class="table">
    <% estate.forEach( function(model) { %>
    <tr>
        <td><%= model.title %></td>
        <td><%= model.estate %></td>
        <td><%= model.area %></td>
        <td><%= model.rent %></td>
        <td><%= model.url %></td>
        <td><%= model.bedroom %></td>
        <td><%= model.tenant %></td>
    </tr>
    <% }); %>
</table>